# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

GET     /v1/users/:id                    controllers.UserController.info(id: Long)
POST    /v1/users/signIn                 controllers.UserController.signIn
POST    /v1/users                        controllers.UserController.singUp

GET     /v1/products/:id                 controllers.ProductController.getOne(id: Long)
GET     /v1/products                     controllers.ProductController.getAll